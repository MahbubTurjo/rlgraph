{
  "type": "single-impala",

  "num_workers": 4,
  "worker_sample_size": 60,

  "visualize": 1,

  "weight_pg": 1.0,
  "weight_vf": 0.5,
  "weight_entropy": 0.00025,

  "fifo_queue_spec":
  {
    "capacity": 1
  },

  "environment_spec":
  {
    "type": "openai-gym",
    "gym_env": "CartPole-v0"
  },

  "feed_previous_action_through_nn": false,
  "feed_previous_reward_through_nn": false,

  "preprocessing_spec":
  [
  ],

  "network_spec":
  {
    "layers": [
      {
        "type": "dense",
        "units": 128,
        "activation": "linear",
        "scope": "layer0"
      },
      {
        "type": "lstm",
        "units": 64,
        "activation": "linear",
        "scope": "layer1"
      }
    ]
  },

  "internal_states_space": {
    "type": "tuple",
    "_args": [
      {
        "type": "float",
        "shape": [64]
      },
      {
        "type": "float",
        "shape": [64]
      }
    ],
    "add_batch_rank": false
  },

  "update_spec": {
    "do_updates": true,
    "batch_size": 32
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.0001
  }
}
