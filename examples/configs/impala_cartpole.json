{
  "type": "single-impala",

  "num_workers": 4,
  "worker_sample_size": 20,

  "environment_spec":
  {
    "type": "openai-gym",
    "gym_env": "CartPole-v0",
    "visualize": true
  },

  "feed_previous_action_through_nn": false,
  "feed_previous_reward_through_nn": false,

  "fifo_queue_spec":
  {
    "capacity": 1
  },

  "network_spec":
  [
      {
        "type": "dense",
        "units": 128,
        "activation": "linear",
        "scope": "hidden-layer"
      }
  ],

  "update_spec": {
    "do_updates": true,
    "batch_size": 16
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.05
  }
}
