{
  "type": "ppo",
  "sample_episodes": false,
  "standardize_advantages": false,
  "clip_ratio": 0.2,
  "discount": 0.99,
  "gae_lambda": 1.0,
  "weight_entropy": 0.01,
  "num_iterations": 10,
  "sample_size": 50

  "python_buffer_size": 200,
  "memory_batch_size": 200,
  "memory_spec":
  {
    "type": "ring_buffer",
    "capacity": 1000
  },

  "preprocessing_spec":
  [],

  "execution_spec": {
    "seed": 15
  },

  "network_spec":
  [
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "hidden1"
    },
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "hidden2"
    }
  ],
  "value_function_spec":
  [
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "vf-hidden1"
    },
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "vf-hidden2"
    }
  ],


  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0025
  },
  "value_function_optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0025
  }
}
