{
  "type": "dqfd",

  "python_buffer_size": 0,

  "memory_batch_size": 32,
  "sync_rules": {
    "sync_every_n_updates": 6
  },
  "memory_spec": {
    "type": "prioritized_replay",
    "capacity": 10000
  },
  "demo_memory_spec":
  {
    "type": "replay",
    "capacity": 10000
  },
  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0005
  },
  "policy_spec":
  {
    "type": "dueling-policy",
    "units_state_value_stream": 128,
    "action_adapter_spec":
    {
      "pre_network_spec": [
        {
          "type": "dense",
          "units": 128
        }
      ]
    }
  },
  "discount": 0.99,
  "exploration_spec":
  {
    "epsilon_spec": {
      "decay_spec": {
        "type": "linear-decay",
        "from": 1.0,
        "to": 0.05
      }
    }
  },
  "demo_sample_ratio": 0.2,
  "expert_margin": 0.1,
  "supervised_weight": 1
}