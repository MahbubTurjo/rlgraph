{
  "type": "dqn",

  "memory_spec": {
    "type": "prioritized-replay",
    "capacity": 256
  },

  "python_buffer_size": 8,

  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "batch_size": 32,
    "sync_interval": 32
  },

  "preprocessing_spec": [
    {
      "type": "convert-type",
      "to_dtype": "float32"
    }
  ],
  "network_spec":
  [
    {
        "type": "dense",
        "units": 32,
        "activation": "relu",
        "scope": "dense-0"

    },
    {
        "type": "dense",
        "units": 32,
        "activation": "relu",
        "scope": "dense-1"
    }
  ],
    "policy_spec":
  {
    "type": "dueling-policy",
    "units_state_value_stream": 16,
    "action_adapter_spec":
    {
      "pre_network_spec": [
        {
          "type": "dense",
          "units": 16
        }
      ]
    }
  },
  "exploration_spec":
  {
    "epsilon_spec": {
      "decay_spec": ["linear", 1.0, 0.05]
    }
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.00025
  }
}
