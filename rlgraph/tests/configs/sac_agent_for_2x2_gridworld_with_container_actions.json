{
  "type": "sac",
  "discount": 0.99,
  "initial_alpha": 0.2,
  "gumbel_softmax_temperature": 1.0,
  "double_q": false,

  "sync_rules": {
    "sync_every_n_updates": 1,
    "sync_tau": 0.005
  },
"memory_batch_size": 32,
  "memory_spec":
  {
    "type": "replay",
    "capacity": 64
  },

  "preprocessing_spec": [
    {
      "type": "convert-type",
      "to_dtype": "float32"
    }
  ],

  "network_spec":
  [
    {
      "type": "dense",
      "units": 32,
      "activation": "relu",
      "scope": "hidden1"
    }
  ],
  "q_function_spec":
  [
    {
      "type": "dense",
      "units": 32,
      "activation": "relu",
      "scope": "vf-hidden1"
    }
  ],

  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0002
  },
  "q_function_optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0004
  }
}
