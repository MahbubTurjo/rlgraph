{
  "type": "single-impala",

  "num_workers": 4,
  "worker_sample_size": 20,
  "visualize": 0,

  "update_spec": {
    "batch_size": 8
  },

  "fifo_queue_spec":
  {
    "capacity": 1
  },

  "environment_spec":
  {
    "type": "open-ai-gym",
    "gym_env": "CartPole-v0"
  },

  "feed_previous_action_through_nn": false,
  "feed_previous_reward_through_nn": false,

  "network_spec":
  {
    "layers": [
      {
        "type": "dense",
        "units": 10,
        "activation": "linear",
        "scope": "hidden"
      }
    ],
    "scope": "impala-net"
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.005
  }
}
