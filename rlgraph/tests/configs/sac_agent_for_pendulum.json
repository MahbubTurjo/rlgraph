{
  "type": "sac",
  "discount": 0.99,
  "initial_alpha": 1.0,
  "double_q": true,

  "memory_spec":
  {
    "type": "replay",
    "capacity": 100000
  },

  "preprocessing_spec":
  [],

  "observe_spec": {
    "buffer_size": 200
  },
  "execution_spec": {
    "seed": 15
  },

  "network_spec":
  [
    {
      "type": "dense",
      "units": 256,
      "activation": "relu",
      "scope": "hidden1"
    },
    {
      "type": "dense",
      "units": 256,
      "activation": "relu",
      "scope": "hidden2"
    }
  ],
  "value_function_spec":
  [
    {
      "type": "dense",
      "units": 256,
      "activation": "relu",
      "scope": "vf-hidden1"
    },
    {
      "type": "dense",
      "units": 256,
      "activation": "relu",
      "scope": "vf-hidden2"
    }
  ],


  "update_spec": {
    "do_updates": true,
    "update_mode": "time_steps",
    "update_interval": 1,
    "sync_interval": 1,
    "sync_tau": 5e-3,
    "batch_size": 256,
    "num_iterations": 1
  },

  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 3e-4
  },
  "value_function_optimizer_spec": {
    "type": "adam",
    "learning_rate": 3e-4
  }
}
