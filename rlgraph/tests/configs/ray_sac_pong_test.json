{
  "type": "sac",
  "discount": 0.99,
  "initial_alpha": 0.2,
  "gumbel_softmax_temperature": 1.0,
  "double_q": false,

  "sync_rules": {
    "sync_interval": 1,
    "sync_tau": 0.005
  },
  "memory_batch_size": 64,
  "memory_spec": {
    "type": "prioritized_replay",
    "capacity": 128
  },
  "saver_spec": null,
  "preprocessing_spec": [
    {
      "type": "grayscale",
      "keep_rank": true,
      "scope": "grayscale"
    },
    {
      "type": "image_resize",
      "width": 84,
      "height": 84,
      "scope": "image_resize"
    },
    {
      "type": "convert_type",
      "to_dtype": "float",
      "scope": "convert_type"
    },
    {
      "type": "divide",
      "divisor": 255.0,
      "scope": "divide"
    },
    {
      "type": "sequence",
      "sequence_length": 4,
      "batch_size": 1,
      "add_rank": false,
      "scope": "sequence"
    }
  ],
  "network_spec": [
    {
      "type": "conv2d",
      "filters": 16,
      "kernel_size": 8,
      "strides": 4,
      "padding": "same",
      "activation": "relu",
      "scope": "conv1"
    },
    {
      "type": "conv2d",
      "filters": 32,
      "kernel_size": 4,
      "strides": 2,
      "padding": "same",
      "activation": "relu",
      "scope": "conv2"
    },
    {
      "type": "conv2d",
      "filters": 256,
      "kernel_size": 11,
      "strides": 1,
      "padding": "valid",
      "activation": "relu",
      "scope": "conv3"
    },
    {
      "type": "reshape",
      "flatten": true
    }
  ],
    "value_function_spec": [
    {
      "type": "conv2d",
      "filters": 16,
      "kernel_size": 8,
      "strides": 4,
      "padding": "same",
      "activation": "relu",
      "scope": "conv1"
    },
    {
      "type": "conv2d",
      "filters": 32,
      "kernel_size": 4,
      "strides": 2,
      "padding": "same",
      "activation": "relu",
      "scope": "conv2"
    },
    {
      "type": "conv2d",
      "filters": 256,
      "kernel_size": 11,
      "strides": 1,
      "padding": "valid",
      "activation": "relu",
      "scope": "conv3"
    },
    {
      "type": "reshape",
      "flatten": true
    }
  ],

  "execution_spec": {
    "gpu_spec": {
      "gpus_enabled" : false,
      "allow_memory_growth": true
    },
    "disable_monitoring": true,
    "session_config": {
		"allow_soft_placement": true,
		"device_count": {
			"CPU": 1
		},
		"inter_op_parallelism_threads": 1,
		"intra_op_parallelism_threads": 1
    },
    "ray_spec": {
      "executor_spec": {
        "redis_address": null,
        "num_cpus": 4,
        "num_gpus": 0,
        "weight_sync_steps": 400,
        "replay_sampling_task_depth": 1,
        "env_interaction_task_depth": 1,
        "num_worker_samples": 200,
        "learn_queue_size": 16,
        "num_cpus_per_worker": 1,
        "num_gpus_per_worker": 0,
        "num_sample_workers": 1,
        "num_replay_workers": 1
      },
      "worker_spec": {
        "execution_spec": {
          "gpu_spec": {
            "gpus_enabled": false
          },
          "disable_monitoring": true,
          "session_config": {
            "allow_soft_placement": true,
            "device_count": {
              "CPU": 1
            },
            "inter_op_parallelism_threads": 1,
            "intra_op_parallelism_threads": 1
          }
        },
        "num_worker_environments": 2,
        "num_background_envs": 0,
        "worker_frame_skip": 1,
        "env_internal_frame_skip": 4,
        "n_step_adjustment": 3,
        "worker_executes_postprocessing": false,
        "sample_exploration": false,
        "ray_constant_exploration": false
      },
      "apex_replay_spec": {
        "memory_spec": {
          "capacity": 1000,
          "alpha": 0.6,
          "beta": 0.4
        },
        "clip_rewards": true,
        "min_sample_memory_size": 100
      }
    }
  },

  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0002,
        "clip_grad_norm": 40

  },
  "value_function_optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.0004
  }

}
